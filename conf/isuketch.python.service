[Unit]
Description = isucon6 final main application in python

[Service]
WorkingDirectory=/home/isucon/webapp/python
EnvironmentFile=/home/isucon/webapp/env.sh

ExecStart = /home/isucon/webapp/python/venv/bin/gunicorn -w 6 -b :8001 app:app

Restart   = always
Type      = simple
User      = isucon
Group     = isucon

[Install]
WantedBy = multi-user.target
